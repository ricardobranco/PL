

=titulo=\[[^=]*\] {
					mybuffer = strdup(yytext+9);
					length = strlen(mybuffer);
					titulo = (char*) malloc(sizeof(char)*length);
					strncpy(titulo,mybuffer,length-1);
					printf("TITULO: %s\n",titulo);
					}


